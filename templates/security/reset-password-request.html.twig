{% extends 'base.html.twig' %}

{% block title %}Réinitialisation de mot de passe{% endblock %}

{% block body %}


    <div class="title gite-title center">
        <h1>Réinialiser mon mot de passe</h1>
    </div>

 
    <div class="registerForm">
            {{ form_start(requestPassForm) }}
            {{ form_row(requestPassForm.email)}}

            <div class="btn-register">
                <button type="submit" class="btn register">Envoyer</button>
            </div>
            {{ form_end(requestPassForm) }}
        </div>

   



{% endblock %}
